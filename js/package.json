{
  "name": "@ubtr/sdk",
  "version": "0.0.7",
  "description": "UBT JS sdk",
  "main": "dist/index.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "keywords": [
    "ubt",
    "blockchain",
    "cryptocurrency"
  ],
  "author": "jaspeen",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --minify",
    "test": "jest",
    "generate": "buf generate",
    "buf": "buf",
    "prepare-publish": "npm run build && shx cp package.json dist && shx cp README.md dist && shx cp ../LICENSE dist && shx sed -i 's/dist\\/index/index/g' dist/package.json",
    "clean-publish": "npm run prepare-publish && cd dist && npm publish --access public && cd .."
  },
  "dependencies": {
    "@protobuf-ts/runtime-rpc": "^2.9.0"
  },
  "devDependencies": {
    "@protobuf-ts/grpc-transport": "^2.9.0",
    "@protobuf-ts/plugin": "^2.9.0",
    "@tsconfig/recommended": "^1.0.2",
    "jest": "^29.5.0",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tsup": "^8.0.1",
    "typescript": "^5.1.3"
  }
}
